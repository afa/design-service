<body>

<noindex> 

<%= render 'layouts/login_popup' %>

<div class="twoColFixRtHdr">
<div class="border_ornements">
<div class="header_top">
<div class="coconut_leaves">

<div id="container">
  <%= render 'layouts/header' %>
  <div id='maincontent_default'>  <!--  ------------------------------------------------------------------------------CHANGE ID NAME -->
    <div class="page_content_default">
      <div class="collage_design mode1">                              
        <%= render 'layouts/specialists' %>
      </div>
      <%= yield %>
      <div id="block_menu_default">
        <%= render 'layouts/worktypes' %>
      </div>
    </div>
  </div> <!--------------------------------------------------- -----------------------------------------------END maincontent_default -->
</div><!--------------------------------------------------- --------------------------------------------------------END container--------->
<%= render "layouts/footer" %>

</div> <!-- coconut_leaves ----------------------------------------------------- END CLASS--->
</div> <!-- header_top  ----------------------------------------------------- END CLASS--->
</div> <!-- border_ornements ---------------------------------------------------- END CLASS-->  
</div>


</noindex>
</body>
</html>
